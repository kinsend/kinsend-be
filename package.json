{
  "name": "kinsendbe-project",
  "private": true,
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@aws-sdk/client-amplify": "^3.112.0",
    "@aws-sdk/client-route-53": "^3.112.0",
    "@aws-sdk/client-s3": "^3.88.0",
    "@aws-sdk/s3-request-presigner": "^3.88.0",
    "@nestjs-modules/mailer": "^1.6.1",
    "@nestjs/axios": "^0.0.7",
    "@nestjs/common": "^8.4.4",
    "@nestjs/config": "^2.0.0",
    "@nestjs/core": "^8.4.4",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/mongoose": "^9.0.3",
    "@nestjs/passport": "^8.2.1",
    "@nestjs/platform-express": "^8.1.1",
    "@nestjs/swagger": "^5.2.1",
    "@sendgrid/mail": "^7.6.2",
    "bcrypt": "^5.0.1",
    "bunyan": "^1.8.15",
    "cache-manager": "^3.6.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "date-and-time": "^2.4.0",
    "dotenv": "^16.0.0",
    "dotenv-expand": "^8.0.3",
    "ejs": "^3.1.6",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-more": "^1.0.5",
    "express-http-context": "^1.2.4",
    "handlebars": "^4.7.7",
    "helmet": "^5.0.2",
    "js-sha256": "^0.9.0",
    "lodash": "^4.17.21",
    "mongoose": "^6.3.0",
    "multer": "^1.4.4",
    "nestjs-mongoose-exclude": "^1.0.5",
    "node-schedule": "^2.1.0",
    "nodemailer": "^6.7.3",
    "on-headers": "^1.0.2",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "randomstring": "^1.2.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.5",
    "stripe": "^8.219.0",
    "swagger-ui-express": "^4.3.0",
    "twilio": "^3.76.1",
    "vcards-js": "^2.10.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.1.3",
    "@nestjs/schematics": "^8.0.4",
    "@nestjs/testing": "^8.1.1",
    "@types/bcrypt": "^5.0.0",
    "@types/bunyan": "^1.8.8",
    "@types/cache-manager": "^3.4.3",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.11.1",
    "@types/node-schedule": "^2.1.0",
    "@types/on-headers": "^1.0.0",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/randomstring": "^1.1.8",
    "@types/supertest": "^2.0.11",
    "@types/vcards-js": "^2.10.2",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-unicorn": "^33.0.1",
    "husky": "^7.0.2",
    "jest": "^27.3.0",
    "jest-circus": "^27.0.6",
    "jest-stare": "^2.2.2",
    "lint-staged": "^11.1.2",
    "prettier": "^2.6.2",
    "prisma": "^3.4.1",
    "rimraf": "^3.0.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.3.0",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "npm run lint -- --cache --fix"
    ],
    "*.js": "eslint --cache --fix"
  }
}
